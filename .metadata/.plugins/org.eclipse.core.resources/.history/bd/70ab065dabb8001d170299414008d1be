package chapter3.B_ValueableMethod;



class Triangle {
	double underSide;
	double height;
	double diagonal;
	
	double area;
	double circumference;
	
	
	
	
	
	
}






public class Method2 {

	
	
	
	
	
	
	public static void main(String[] args) {
		Triangle triangle1 = new Triangle();
		triangle1.diagonal = 5;
		triangle1.height = 4;
		triangle1.underSide =3;
		
		

	}

}
